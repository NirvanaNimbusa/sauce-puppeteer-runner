#!/usr/bin/env node

const child_process = require('child_process');
const { createWriteStream } = require('fs');

(async () => {
    const child = child_process.spawn('script',
        ['-q', '-f', '-e', '-c',
        ['jest', ...process.argv.slice(2)].join(" "),
        'puppeteer.typescript'], { stdio: 'inherit' });
})();
